{% extends "layoutAdmin.njk" %}

{% block content %}
<div id="first-page">    
    <h3>Gerenciar Chefs</h3>
    <div class="cards-chef">
    {% for chef in chefs %}
        <div class="chef-index">
            <img src="{{chef.img}}" alt="">
            <div class="texts">
                <p>{{chef.name}}</p>
                <a href="/admin/chefs/{{chef.id}}">Visualizar</a>
            </div>
        </div>
    {% endfor %}
    </div>
</div>
{% endblock content %}