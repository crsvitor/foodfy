{% extends "layout.njk" %}

{% block content %}
<section class="recipes">
    {% for chef in chefs %}
            <div class="chef" id="{{chef.id}}">
                <img src="{{chef.avatar_url}}" alt="">
                <h3>{{chef.name}}</h3>
                <p>{{chef.recipes}}</p>
            </div>
    {% endfor %}
</section>
{% endblock content %}